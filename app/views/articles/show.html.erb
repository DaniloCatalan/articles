<%= form_with(model: @article, local: true) do |f| %>
  <label for="ean">Code</label>
  <div><%= @article.ean%></div>
  <br>
  <label for="name">Name</label>
  <div><%= @article.name%></div>
  <br>
  <label for="Price">Price</label>
    <div><%= @article.price%></div>
  <br>
  <%= link_to "Edit", edit_article_path(@article) %>
