<h1>Articles</h1>
<table>
  <thead>
    <th>Ean</th>
    <th>Name</th>
    <th>Price</th>
    <th>Actions</th>
  </thead>
  <tbody>
    <% @articles.each do |article| %>
    <tr>
      <td><%= article.ean %></td>
      <td><%= article.name %></td>
      <td><%= article.price %></td>
      <td><%= link_to "Show", article_path(article) %>
    </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "New Article", new_article_path %>

