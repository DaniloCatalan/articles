
<h1>Edit Product</h1>
<%= form_with(model: @product) do |f| %>
  <label for="ean">Code</label>
  <%= f.text_field :ean %>
<br>
<label for="name">Name</label>
  <%= f.text_field :name %>
<br>
<label for="Price">Price</label>
  <%= f.text_field :price %>
<br>
 <%= f.submit %>
<% end %>
<%= link_to "Back to index", products_path %>

