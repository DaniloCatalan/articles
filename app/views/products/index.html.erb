<h1>Products</h1>
<table>
  <thead>
    <th>Ean</th>
    <th>Name</th>
    <th>Price</th>
    <th>Actions</th>
  </thead>
  <tbody>
    <% @products.each do |product| %>
    <tr>
      <td><%= product.ean %></td>
      <td><%= product.name %></td>
      <td><%= product.price %></td>
      <td><%= link_to "Show", product_path(product) %>
    </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "New Article", new_product_path %>

